{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Omnipair Docs",
  "colors": {
    "primary": "#00E898",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "index",
              "introduction",
              "omnipair/problem",
              "omnipair/solution"
            ]
          },
          {
            "group": "Technical breakdown",
            "pages": [
              "technical-breakdown/overview",
              "technical-breakdown/generalized-automated-market-maker",
              "technical-breakdown/slippage-aware-collateral-factor",
              "technical-breakdown/liquidations-debt-write-off",
              "technical-breakdown/risk-and-safe-design",
              "technical-breakdown/customizable-pools"
            ]
          },
          {
            "group": "Governance",
            "pages": [
              "governance/what-is-omfg",
              "governance/governance-model",
              "governance/protocol-governable-parameters"
            ]
          },
          {
            "group": "Use cases",
            "pages": [
              "use-cases/leveraging",
              "use-cases/borrowing",
              "use-cases/liquidity-provisioning-earn"
            ]
          }
        ]
      },
      {
        "tab": "Code Reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Launch App",
          "href": "https://app.omnipair.fi",
          "icon": "arrow-up-right-from-square"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/omnipair",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/omnipair",
          "icon": "discord"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://discord.gg/omnipair"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch App",
      "href": "https://app.omnipair.fi"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/omnipair",
      "github": "https://github.com/omnipair"
    }
  }
}